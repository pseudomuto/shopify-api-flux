{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "indent": [2, 2],
    "quotes": [2, "double"],
    "semi": [2, "always"],
    "object-shorthand": [2, "always"],
    "no-console": 1
  },
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {
    "fetch"
  }
}
