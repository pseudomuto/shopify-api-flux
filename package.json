  "devDependencies": {
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.0",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "eslint": "^1.2.1",
    "jest-cli": "^0.5.0",
    "webpack": "^1.12.0"
  },
  "dependencies": {
    "flux": "^2.1.1",
    "keymirror": "^0.1.1",
    "lodash": "^3.10.1",
    "object-assign": "^4.0.1",
    "whatwg-fetch": "^0.9.0"
  }
